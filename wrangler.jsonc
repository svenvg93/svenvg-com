{
  "name": "svenvg-com",
  "compatibility_date": "2025-07-20",
  "assets": {
    "directory": "./public",
    "html_handling": "auto-trailing-slash",
    "not_found_handling": "404-page",
    "run_worker_first": false
  },
  "build": {
    "command": "hugo build --gc --minify",
  },
  "workers_dev": true,
  "preview_urls": true,
  "routes": [
    {
        "custom_domain": true,
        "pattern": "svenvg.com"
    }
  ]
}
